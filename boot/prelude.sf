Nat : TYPE
Nat = (P : TYPE) -> P -> (P -> P) -> P

zeroN : Nat
zeroN N z s = z

succN : Nat -> Nat
succN n N z s = s (n N z s)

rec : (N : TYPE) -> N -> (N -> N) -> Nat -> N
rec N z s n = n N z s

addN : Nat -> Nat -> Nat
addN x y = rec Nat x succN y

add-zero-is-zero : Nat -> Nat
add-zero-is-zero x = addN x zeroN

add-succ-is-succ : Nat -> Nat -> Nat
add-succ-is-succ x y = addN x (succN y)

foo : TYPE -> TYPE
foo T = {
  bar : TYPE;
  bar = T;

  bar
} 


